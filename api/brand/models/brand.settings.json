{
  "kind": "collectionType",
  "collectionName": "brands",
  "info": {
    "name": "Brand"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "BrandName": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid"
    },
    "Website": {
      "type": "string"
    },
    "Email": {
      "type": "string"
    },
    "Phone": {
      "type": "string"
    },
    "Twitter": {
      "type": "string"
    },
    "Instagram": {
      "type": "string"
    },
    "Facebook": {
      "type": "string"
    },
    "Products": {
      "collection": "product",
      "via": "Brand"
    },
    "BrandLogo": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    }
  }
}
