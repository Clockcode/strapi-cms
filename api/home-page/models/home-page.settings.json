{
  "kind": "singleType",
  "collectionName": "home_pages",
  "info": {
    "name": "Home Page"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Seo": {
      "type": "component",
      "repeatable": false,
      "component": "config.seo"
    },
    "HomePageTitle": {
      "type": "string"
    },
    "Description": {
      "type": "string"
    },
    "product": {
      "collection": "product"
    },
    "Footer": {
      "type": "component",
      "repeatable": false,
      "component": "config.footer"
    },
    "NavigationMenu": {
      "type": "component",
      "repeatable": false,
      "component": "config.navigation-menu"
    },
    "Banner": {
      "type": "component",
      "repeatable": true,
      "component": "config.banner-image"
    },
    "SectionBelow": {
      "type": "component",
      "repeatable": true,
      "component": "config.section-below",
      "max": 2
    }
  }
}
